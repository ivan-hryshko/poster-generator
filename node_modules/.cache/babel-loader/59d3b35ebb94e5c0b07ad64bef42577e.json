{"ast":null,"code":"import { ref, computed } from 'vue';\nimport dataset from '@/assets/DataSet/13639-Metadata.json';\nimport axios from 'axios';\nimport SpaceImage from '@/components/space-image';\nimport { mapMutations } from 'vuex';\nexport default {\n  name: 'ChoosePicture',\n  components: {\n    SpaceImage\n  },\n\n  setup() {\n    console.log('fakeData.value :>> ', fakeData.value); // const url = 'http://localhost:8000/api/'\n    // const url = 'https://www.random.org/'\n\n    const url = 'http://44.202.137.22/api';\n    const client = axios.create({\n      baseURL: url,\n      timeout: 1000\n    });\n    return {\n      dataset,\n      combinedList,\n      fullData,\n      isRealData,\n      fakeData\n    };\n  },\n\n  data: () => ({\n    width: 900\n  }),\n  computed: {\n    fullData: function () {\n      let newArray = [];\n      newArray = combinedList.value.slice();\n\n      for (const combinedItem of newArray) {\n        for (const imageGroup of imageList.value) {\n          if (imageGroup.id === combinedItem.from_images) combinedItem.metadata = imageGroup.metadata;\n          combinedItem.image_b = imageGroup.image_b;\n          combinedItem.image_g = imageGroup.image_g;\n          combinedItem.image_r = imageGroup.image_r;\n        } // combinedItem.from_images\n\n      }\n\n      console.log('newArray :>> ', newArray);\n      return newArray;\n    }\n  },\n  methods: { ...mapMutations(['saveFullData' // map `this.increment()` to `this.$store.commit('increment')`\n    ]),\n\n    getRequest() {\n      isRealData.value = true;\n      getImageList();\n      getCombinedList();\n    },\n\n    async getImageList() {\n      imageList.value = await client.get('/rgb_list');\n      imageList.value = imageList.value.data;\n      console.log('imageList.value :>> ', imageList.value);\n    },\n\n    async getCombinedList() {\n      combinedList.value = await client.get('/combined_list');\n      combinedList.value = combinedList.value.data;\n      console.log('combinedList.value :>> ', combinedList.value);\n    }\n\n  },\n\n  mounted() {\n    // this.saveFullData(this.fullData)\n    this.getRequest();\n    console.log('this.$store.state.fullDatas :>> ', this.$store.state.fullDatas);\n  }\n\n};","map":{"version":3,"mappings":"AAqCA;AACA;AACA;AACA;AACA;AAEA;EACAA,qBADA;EAGAC;IACAC;EADA,CAHA;;EAMAC;IAIAC,mDAJA,CAOA;IACA;;IACA;IACA;MACAC,YADA;MAEAC;IAFA;IAMA;MACAC,OADA;MAEAC,YAFA;MAGAC,QAHA;MAIAC,UAJA;MAKAC;IALA;EAOA,CA7BA;;EA8BAC;IACAC;EADA,EA9BA;EAiCAC;IACAL;MACA;MACAM;;MACA;QACA;UACA,gDACAC;UACAA;UACAA;UACAA;QACA,CAPA,CAQA;;MACA;;MACAZ;MACA;IACA;EAhBA,CAjCA;EAmDAa,WACA,iBACA,cADA,CACA;IADA,EADA;;IAIAC;MACAR;MAEAS;MACAC;IACA,CATA;;IAUA;MACAC;MACAA;MACAjB;IACA,CAdA;;IAeA;MACAI;MACAA;MACAJ;IACA;;EAnBA,CAnDA;;EAwEAkB;IACA;IACA;IACAlB;EAEA;;AA7EA","names":["name","components","SpaceImage","setup","console","baseURL","timeout","dataset","combinedList","fullData","isRealData","fakeData","data","width","computed","newArray","combinedItem","methods","getRequest","getImageList","getCombinedList","imageList","mounted"],"sourceRoot":"src/views","sources":["ChoosePicture.vue"],"sourcesContent":["<template>\n  <div\n    class=\"main-content\"\n  >\n    <div class=\"content__header\">\n      <h2 class=\"header-title\">Step 1. Angle</h2>\n      <div class=\"header-btns\">\n        <div class=\"btn\"><h5 class=\"btn__text\"> Cancel</h5></div>\n        <div class=\"btn btn-border\"><h5 class=\"btn__text\"> Preview</h5></div>\n      </div>\n    </div>\n    <div\n      v-if=\"isRealData\"\n      class=\"gallery\"\n    >\n      <SpaceImage\n        v-for=\"item, index in fullData\"\n        :key=\"index\"\n        :image-item=\"item\"\n        :is-real-data=\"isRealData\"\n      />\n    </div>\n    <div\n      v-else\n      class=\"gallery\"\n    >\n      <SpaceImage\n        v-for=\"item, index in fakeData\"\n        :key=\"index\"\n        :image-item=\"item\"\n        :is-real-data=\"isRealData\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\n  import { ref, computed } from 'vue'\n  import dataset from '@/assets/DataSet/13639-Metadata.json'\n  import axios from 'axios'\n  import SpaceImage from '@/components/space-image'\n  import { mapMutations } from 'vuex'\n\n  export default {\n    name: 'ChoosePicture',\n\n    components: {\n      SpaceImage,\n    },\n    setup() {\n\n \n\n      console.log('fakeData.value :>> ', fakeData.value);\n\n\n      // const url = 'http://localhost:8000/api/'\n      // const url = 'https://www.random.org/'\n      const url = 'http://44.202.137.22/api'\n      const client = axios.create({\n        baseURL: url,\n        timeout: 1000,\n      });\n\n\n      return {\n        dataset,\n        combinedList,\n        fullData,\n        isRealData,\n        fakeData,\n      }\n    },\n    data: () => ({\n      width: 900,\n    }),\n    computed: {\n      fullData: function() {\n        let newArray = []\n        newArray = combinedList.value.slice()\n        for (const combinedItem of newArray) {\n          for (const imageGroup of imageList.value) {\n            if (imageGroup.id === combinedItem.from_images)\n            combinedItem.metadata = imageGroup.metadata\n            combinedItem.image_b = imageGroup.image_b\n            combinedItem.image_g = imageGroup.image_g\n            combinedItem.image_r = imageGroup.image_r\n          }\n          // combinedItem.from_images\n        }\n        console.log('newArray :>> ', newArray);\n        return newArray\n      }\n    },\n    methods: {\n      ...mapMutations([\n        'saveFullData', // map `this.increment()` to `this.$store.commit('increment')`\n      ]),\n      getRequest() {\n        isRealData.value = true\n\n        getImageList()\n        getCombinedList()\n      },\n      async getImageList() {\n        imageList.value = await client.get('/rgb_list')\n        imageList.value = imageList.value.data\n        console.log('imageList.value :>> ', imageList.value);\n      },\n      async getCombinedList() {\n        combinedList.value = await client.get('/combined_list')\n        combinedList.value = combinedList.value.data\n        console.log('combinedList.value :>> ', combinedList.value);\n      },\n    },\n    mounted() {\n      // this.saveFullData(this.fullData)\n      this.getRequest()\n      console.log('this.$store.state.fullDatas :>> ', this.$store.state.fullDatas);\n\n    }\n  }\n</script>\n\n<style>\n.main-content {\n    /* width: 100%; */\n    padding: 0 64px;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n  }\n\n  .content__header{\n    width: 100%;\n    display: flex;\n    justify-content: space-between;\n    margin: 45px 0;\n  }\n\n  .header-title{\n    font-weight: normal;\n    font-size: 48px;\n    line-height: 62px;\n  }\n\n  .header-btns{\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n\n  .btn{\n    border-radius: 16px;\n    cursor: pointer;\n  }\n\n  .btn__text{\n    margin: 18px 43px;\n    font-weight: 700;\n    font-size: 14px;\n    line-height: 20px;\n  }\n\n  .btn-border{\n    border: 1px solid #702DFF;\n  }\n\n  .content__header .btn-border {\n    margin-left: 8px;\n  }\n\n  .slider-wrapped {\n\n  }\n\n  .gallery {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    row-gap: 20px;\n    column-gap: 40px;\n  }\n\n  .image {\n    width: 600px;\n\n  }\n\n  @media (max-width:1399px)  {\n    .gallery {\n      grid-template-columns: 1fr 1fr;\n    }\n  }\n\n  @media (max-width:1000px)  {\n    .gallery {\n      grid-template-columns: 1fr;\n    }\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}